{
  "name": "PrintMQTTify",
  "version": "1.0.0",
  "slug": "printmqttify",
  "description": "Bridges MQTT messages to a CUPS printer for Home Assistant.",
  "arch": ["amd64", "armv7", "arm64"],
  "startup": "services",
  "hassio_api": true,
  "hassio_role": "default",
  "ports": {
    "631/tcp": 631
  },
  "host_network": false,
  "privileged": ["SYS_ADMIN", "NET_ADMIN"],
  "options": {
    "mqtt_broker": "127.0.0.1",
    "mqtt_username": "admin",
    "mqtt_password": "password"
  },
  "schema": {
    "mqtt_broker": "str",
    "mqtt_username": "str",
    "mqtt_password": "str"
  }
}
